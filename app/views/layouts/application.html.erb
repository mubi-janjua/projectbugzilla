<!DOCTYPE html>
<html>
  <head>
    <title>Project</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <div class="container">
      <%= render "layouts/navbar" %><br>
      <div class="image">


        <% flash.each do |name,msg| %>
        <ul>
          <li><%= msg%></li>
        </ul>
       <%end%>
      <%= yield %>
    </div>
    </div>
  </body>
</html>
